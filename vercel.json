{
	"crons": [
		{
			"path": "/api/cron",
			"schedule": "0 19 * * *"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "no-store"
				},
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "no-store"
				},
				{
					"key": "Pragma",
					"value": "no-cache"
				},
				{
					"key": "Expires",
					"value": "0"
				}
			]
		}
	]
}
